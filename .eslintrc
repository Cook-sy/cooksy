{
  "env": {
    "browser": false,
    "node": true
  },
  "rules": {
    "func-names": ["error", "never"],
    "no-console": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "no-shadow": "warn",
    "no-unused-vars": ["error", { "args": "none" }],
    "space-before-function-paren": ["error", "never"],
    "vars-on-top": "off"
  },
  "extends": ["airbnb-base/legacy"]
}
